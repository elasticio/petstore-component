{
  "title": "Additional Field Test",
  "description": "elastic.io component for the Petstore API",
  "docsUrl": "https://github.com/elasticio/petstore-component-nodejs",
  "buildType":"docker",
  "authClientTypes": ["oauth2"],
  "credentials": {
    "fields": {
      "auth": {
        "required": true,
        "viewClass": "HTTPAuthView"
      },
      "url": {
        "label": "URL",
